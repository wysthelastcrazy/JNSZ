<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@color/common_grey">

    <LinearLayout
        android:id="@+id/layout_header"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@drawable/btn"
        android:gravity="center"
        android:padding="2px" >

        <TextView
            android:id="@+id/textView_daiBan"
            android:layout_width="100dp"
            android:layout_height="35dp"
            android:background="@drawable/bg_myfocus"
            android:gravity="center"
            android:text="合同"
            android:textColor="@drawable/textcolor_main"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/textView_yiBan"
            android:layout_width="100dp"
            android:layout_height="35dp"
            android:background="@drawable/bg_myfocus"
            android:gravity="center"
            android:text="收款"
            android:textColor="@drawable/textcolor_main"
            android:textSize="12sp" />
    </LinearLayout>
    <com.DCHZ.TYLINCN.component.BaoBiaoListTopView 
        android:id="@+id/topView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/layout_header"
        android:layout_marginTop="8dp"
        android:background="@color/common_white"></com.DCHZ.TYLINCN.component.BaoBiaoListTopView>
    
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/common_white"
        android:layout_below="@id/topView"
        android:orientation="vertical">
        <com.DCHZ.TYLINCN.msglist.MsgPage 
        android:id="@+id/mMsgPage_three"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12.5dp"
        android:layout_marginRight="12.5dp"
        android:layout_marginLeft="12.5dp"
        android:padding="1dp"
        android:background="@drawable/bg_third_list"></com.DCHZ.TYLINCN.msglist.MsgPage>\
    </LinearLayout>
    
</RelativeLayout>