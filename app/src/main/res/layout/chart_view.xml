<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/text_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12.5dp"
        android:layout_gravity="center_horizontal"
        android:textSize="16sp"
        android:textColor="#555555"
        android:text="2014合同信息查勋"
        />
    <com.DCHZ.TYLINCN.component.MyPieChart
        android:id="@+id/mPieChart"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.DCHZ.TYLINCN.component.MyPieChart>
    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/mLineChart1"
        android:layout_width="match_parent"
        android:layout_height="220dp"
        android:paddingTop="20dp"></com.github.mikephil.charting.charts.LineChart>
    <TextView
        android:id="@+id/text_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12.5dp"
        android:layout_gravity="center_horizontal"
        android:textSize="14sp"
        android:textColor="#555555"
        android:text="2014合同信息查勋"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/common_grey"
        android:layout_marginTop="6dp"></View>
</LinearLayout>